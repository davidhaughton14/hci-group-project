<link rel="stylesheet" href="/stylesheets/dashboard.css">
<div class = "col-md-6">
    <div class = "summary-wrap">
        <h3 class="text-center heading"> Todays summary </h3>
        <div class = "row">
            <div class = "col-md-12">
                <div id="sleep-heading-wrap">
                    <p class="pull-right" id="sleepCount">You got 6.8/8 hours sleep last night </p>
                </div>
            </div>
        </div>
        <div class = "row">
            <div class = "col-md-12">
                <div class="sleep-wrap">
                    <canvas id="sleep-progress" height="40" width="400"></canvas>
                </div>
            </div>
        </div>
        <div class = "row">
            <div class = "col-md-12">
                <div id="step-heading-wrap">
                    <p class="pull-right" id="stepCount"> 7,342/10,000 steps </p>
                </div>
            </div>
        </div>
        <div class = "row">
            <div class = "col-md-12">
                <div class="step-wrap">
                    <canvas id="steps-progress" height="40" width="400"></canvas>
                </div>
            </div>
        </div>

        <div class = "row">
            <div class = "col-md-12">
                <div id="bp-heading-wrap">
                    <span class="small-text">Your average blood pressure: </span><span class="big-text pull-right">120/80mmHg </span>
                </div>
            </div>
        </div>

        <div class = "row">
            <div class = "col-md-12">
                <div id="vitD-heading-wrap">
                    <span class="small-text">Your average vitamin D levels: </span><span class="big-text pull-right">20 ng/ml </span>
                </div>
            </div>
        </div>

        <div class = "row">
            <div class = "col-md-12">
                <div id="vitD-heading-wrap">
                    <span class="big-text">1 upcoming meetup: </span>
                </div>
            </div>
        </div>
        <div class = "row">
            <div class = "col-md-9 col-md-offset-2">
                <div id="meetup-wrap">
                    <p class="text-center"> At 3pm (1 hour away) <span class="fa-stack fa-lg"> <i class="fa fa-circle fa-stack-1x icon-background1"></i><i class="fa fa-exclamation fa-stack-1x"></i></span></p>
                </div>
            </div>
        </div>
    </div>
</div>

<div class = "col-md-6">
    <div class = "diary-wrap">
        <h3 class="text-center heading"> Todays diary </h3>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.2.1.js"></script>
<script src="javascripts/dashboard.js"></script>
